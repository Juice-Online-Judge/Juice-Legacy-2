    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.4",
    "koa-generic-session": "^1.10.1",
    "koa-json": "^1.1.1",
    "koa-passport": "^2.0.1",
    "koa-redis": "^2.0.1",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "lodash": "^4.5.1",
    "material-ui": "^0.16.7",
    "mysql": "^2.10.2",
